#include<iostream>
using namespace std;

int main(){
	int n,m0,m1,m2;
	cin>>n;
	m0 = n / 50;
	m1 = (n - 50 * m0) / 30;
	m2 = (n - 50 * m0 - 30 * m1) / 10;
	cout<<m0*7+m1*4+m2;
	return 0;
}




